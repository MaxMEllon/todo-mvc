{
  "request": {
    "path": "/todos/:id",
    "method": "PATCH",
    "body": {
      "name": "{:name}",
      "done": "{:done}"
    },
    "values": {
      "id": 1,
      "name": "buy 2 milks"
    }
  },
  "response": {
    "status": 201,
    "body": {
      "id": "{:id}",
      "name": "{:name}",
      "done": "{:done}"
    },
    "values": {
      "id": 1
    }
  }
}
